{
  "name": "alpha-analyzer",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "scripts/bulletproof-start.sh",
    "dev:force": "scripts/bulletproof-start.sh --force-cleanup",
    "dev:verbose": "scripts/bulletproof-start.sh --verbose",
    "dev:dry-run": "scripts/bulletproof-start.sh --dry-run",
    "dev:quick": "scripts/quick-start.sh",
    "dev:legacy": "concurrently \"npm run backend\" \"npm run frontend\"",
    "dev:multi": "concurrently \"npm run backend\" \"npm run frontend:3000\" \"npm run frontend:3005\" \"npm run frontend:8080\"",
    "dev:ultra": "concurrently \"npm run backend\" \"npm run frontend:3000\" \"npm run frontend:3005\" \"npm run frontend:8080\" \"npm run backend:3002\"",
    "backend": "PORT=3001 tsx --env-file=.env server/index.ts",
    "backend:3002": "PORT=3002 tsx --env-file=.env server/index.ts",
    "frontend": "vite --port 3000",
    "frontend:3000": "vite --port 3000",
    "frontend:3005": "vite --config vite.config.port3005.ts",
    "frontend:8080": "vite --config vite.config.port8080.ts",
    "stop": "scripts/bulletproof-stop.sh",
    "stop:force": "scripts/bulletproof-stop.sh --force",
    "stop:clean": "scripts/bulletproof-stop.sh --cleanup-logs",
    "restart": "npm run stop && npm run dev",
    "restart:force": "npm run stop:force && npm run dev:force",
    "port:check": "scripts/port-manager.sh report",
    "port:cleanup": "scripts/port-manager.sh cleanup",
    "port:cleanup:force": "scripts/port-manager.sh cleanup --force",
    "port:watch": "scripts/port-manager.sh watch",
    "port:reserve": "scripts/port-manager.sh reserve",
    "health": "curl -s http://localhost:3001/api/health | jq .",
    "health:detailed": "curl -s http://localhost:3001/api/health/detailed | jq .",
    "health:quick": "curl -s http://localhost:3001/api/health/quick | jq .",
    "health:metrics": "curl -s http://localhost:3001/api/health/metrics | jq .",
    "health:check": "scripts/health-check.sh",
    "health:monitor": "scripts/connectivity-test.ts",
    "test:health": "node test-health-endpoint.cjs",
    "recover": "curl -X POST http://localhost:3001/api/health/recover",
    "access:test": "concurrently \"curl -s http://localhost:3000/api/health || echo 'Port 3000 failed'\" \"curl -s http://localhost:3005/api/health || echo 'Port 3005 failed'\" \"curl -s http://localhost:8080/api/health || echo 'Port 8080 failed'\"",
    "tunnel:simple": "node scripts/simple-tunnel.js start 3000 8090",
    "tunnel:dev": "node scripts/simple-tunnel.js start 3005 8091",
    "tunnel:ngrok": "bash scripts/setup-tunneling.sh ngrok 3000",
    "tunnel:lt": "bash scripts/setup-tunneling.sh localtunnel 3000",
    "tunnel:multi": "bash scripts/setup-tunneling.sh multi",
    "tunnel:status": "bash scripts/setup-tunneling.sh status",
    "tunnel:stop": "bash scripts/setup-tunneling.sh stop",
    "docker:build": "bash scripts/docker-manager.sh build",
    "docker:dev": "bash scripts/docker-manager.sh dev",
    "docker:minimal": "bash scripts/docker-manager.sh minimal",
    "docker:prod": "bash scripts/docker-manager.sh prod",
    "docker:status": "bash scripts/docker-manager.sh status",
    "docker:logs": "bash scripts/docker-manager.sh logs",
    "docker:shell": "bash scripts/docker-manager.sh shell",
    "docker:stop": "bash scripts/docker-manager.sh stop",
    "docker:cleanup": "bash scripts/docker-manager.sh cleanup",
    "build": "vite build --outDir dist/public",
    "build:multi": "concurrently \"vite build --outDir dist/public-3000\" \"vite build --config vite.config.port3005.ts\" \"vite build --config vite.config.port8080.ts\"",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "migrate": "tsx server/db/migrations.ts up",
    "migrate:down": "tsx server/db/migrations.ts down",
    "migrate:status": "tsx server/db/migrations.ts status",
    "migrate:rollback": "tsx server/db/migrations.ts down 1",
    "db:test": "tsx server/db/test-connection.ts",
    "db:check": "tsx server/db/check-tables.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "jest security-tests.test.ts",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:api": "node test-api-connection.js",
    "test:ports": "npm run test:api",
    "test:cors": "npm run test:api",
    "test:connectivity": "scripts/connectivity-test.ts",
    "env:setup": "node setup-environment.js",
    "env:validate": "node validate-environment.js",
    "env:diagnostic": "node env-diagnostic.js",
    "emergency": "node emergency-startup.cjs",
    "emergency:js": "node server/emergency-server.cjs",
    "emergency:python": "python3 server/emergency-python-server.py",
    "emergency:php": "php -S localhost:3001 server/emergency-php-server.php",
    "fix": "npm run emergency",
    "logs": "tail -f logs/backend.log",
    "logs:frontend": "tail -f logs/frontend.log",
    "logs:all": "tail -f logs/*.log",
    "status": "echo 'üîç Alfalyzer Status Check' && npm run port:check && echo '' && npm run health:quick",
    "debug": "echo 'üêõ Debug Information' && npm run status && echo '' && npm run health:detailed",
    "troubleshoot": "echo 'üîß Troubleshooting Guide' && scripts/health-check.sh && echo '' && scripts/connectivity-test.ts"
  },
